#include<stdio.h>
void main()
{
        int a[8]={23,54,76,87,13,49,98,37};
        int i,j,temp;
        for(i=0;i<8;i++)
        {
                for(j=i;j<8;j++)
                {
                        if(a[i]<a[j])
                        {
                                temp=a[i];
                                a[i]=a[j];
                                a[j]=temp;
                        }
                }
        }
        printf("Descending Order : ");
        for(i=0;i<8;i++)
                printf("%d ",a[i]);
        printf("\n");
}
