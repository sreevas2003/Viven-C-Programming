#include<stdio.h>
void main()
{
        int s,i,flag=0;
        printf("Enter size : ");
        scanf("%d",&s);
        int a[s],b[s];
        for(i=0;i<s;i++)
                scanf("%d",&a[i]);
        for(i=0;i<s;i++)
                scanf("%d",&b[i]);
        for(i=0;i<s;i++)
        {
                if(a[i]!=b[i])
                {
                        flag=1;
                        break;
                }
        }
        if(flag==0)
                printf("Two arrays are Equal\n");
        else
                printf("Not Equal\n");
}
