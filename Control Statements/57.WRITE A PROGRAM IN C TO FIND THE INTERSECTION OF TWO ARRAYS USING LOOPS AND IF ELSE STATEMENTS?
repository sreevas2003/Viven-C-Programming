#include<stdio.h>
void main()
{
        int i,flag,j,k=0,a[5],b[4];
        printf("Enter array A of 5 elements: ");
        for(i=0;i<5;i++)
                scanf("%d",&a[i]);
        printf("Enter array B of 4 elemnts: ");
        for(i=0;i<4;i++)
                scanf("%d",&b[i]);
        for(i=0;i<5;i++)
        {
                for(j=0;j<4;j++)
                {
                        if(a[i]==b[j])
                                printf("%d ",a[i]);
                }
        }
        printf("\n");
}
