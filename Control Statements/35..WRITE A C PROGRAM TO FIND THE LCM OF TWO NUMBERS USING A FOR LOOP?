#include<stdio.h>
void main()
{
        int max,a,b;
        printf("Enter two numbers : ");
        scanf("%d%d",&a,&b);
        max=(a>b)?a:b;
        for(;1;max++)
        {
                if(max%a==0 && max%b==0)
                        break;
        }
        printf("%d and %d lcm is %d\n",a,b,max);
}
