#include<stdio.h>
void main()
{
        int a,b,c;
        printf("Enter Three Values");
        scanf("%d%d%d",&a,&b,&c);
        if(a>b && a>c)
                printf("A is greater number\n");
        else if(b>c)
                printf("B is greater number\n");
        else
                printf("C is greater number\n");
}
