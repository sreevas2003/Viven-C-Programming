#include<stdio.h>
void main()
{
        int a[8]={21,34,65,78,93,88,49,55};
        int i,max,smax;
        max=smax=a[0];
        for(i=0;i<8;i++)
        {
                if(max<a[i])
                        max=a[i];
                if(smax<a[i] && max!=a[i])
                        smax=a[i];
        }
        printf("Max=%d second max=%d\n",max,smax);
}
