#include<stdio.h>
void main()
{
        int i;
        int a[9];
        for(i=0;i<9;i++)
        {
                printf("Enter %d element : ",i+1);
                scanf("%d",&a[i]);
        }
        printf("\n");
        for(i=0;i<9;i++)
                printf("%d ",a[i]);
        printf("\n");
}
