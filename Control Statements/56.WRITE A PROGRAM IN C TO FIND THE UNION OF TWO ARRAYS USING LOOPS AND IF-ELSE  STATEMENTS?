#include<stdio.h>
void main()
{
        int a[6]={1,2,3,4,5,6};
        int b[4]={1,5,7,8};
        int i,j,k=0,flag,temp[5];
        for(i=0;i<6;i++)
        {
                flag=0;
                for(j=0;j<4;j++)
                {
                        if(a[i]==b[j])
                        {
                                flag=1;
                                break;
                        }
                }
                if(flag==0)
                        temp[k++]=a[i];
        }
        for(i=0;i<k;i++)
                printf("%d ",temp[i]);
        printf("\n");
}
