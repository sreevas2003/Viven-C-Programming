//row sum
#include<stdio.h>
void main()
{
        int a[3][3]={1,9,2,8,3,7,4,6,5};
        int i,j,sum;
        for(i=0;i<3;i++)
        {
                sum=0;
                for(j=0;j<3;j++)
                {
                        sum+=a[i][j];
                }
                printf("%d row of sum is %d\n",i+1,sum);
        }
}
