#include<stdio.h>
void main()
{
        int a[8]={5,8,9,5,6,4,5,8};
        int i=0,j,ele,c,pc=0;
        int temp[8];
        while(i<8)
                temp[i++]=-1;
        for(i=0;i<8;i++)
        {
                if(temp[j]==-1)
                {
                        c=0;
                        for(j=0;j<8;j++)
                        {
                                if(a[i]==a[j])
                                {
                                        c++;
                                        temp[j]=0;
                                }
                        }
                        if(pc<c)
                        {
                                ele=a[i];
                                pc=c;
                        }
                }
        }
        printf("Majority element is %d is %d times\n",ele,c);
}
