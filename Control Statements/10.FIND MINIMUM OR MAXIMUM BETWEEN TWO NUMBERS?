#include<stdio.h>
void main()
{
        int a,b;
        printf("Enter any Number : ");
        scanf("%d%d",&a,&b);
        if(a==b)
                printf("Both are Equal\n");
        else if(a>b)
                printf("%d is maximum and %d is minimum\n",a,b);
        else
                printf("%d is maximum and %d is minimum\n",b,a);
}
