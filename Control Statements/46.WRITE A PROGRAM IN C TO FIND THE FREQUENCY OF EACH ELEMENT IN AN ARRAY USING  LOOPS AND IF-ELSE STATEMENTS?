#include<stdio.h>
void main()
{
        int a[8]={23,54,23,76,87,54,65,23};
        int freq,i=0;
        int j,temp[8];
        while(i<8)
                temp[i++]=-1;

        for(i=0;i<8;i++)
        {
                if(temp[i]==-1)
                {
                        freq=0;
                        for(j=i;j<8;j++)
                        {
                                if(a[i]==a[j])
                                {
                                        freq++;
                                        temp[j]=0;
                                }
                        }
                        printf("%d is : %d\n",a[i],freq);
                }
        }
}
