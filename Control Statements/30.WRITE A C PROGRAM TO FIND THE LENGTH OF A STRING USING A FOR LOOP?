#include<stdio.h>
#include<string.h>
void main()
{
        char str[15];
        printf("Enter a string : ");
        fgets(str,15,stdin);
        int i,n=strlen(str),l=0;
        for(i=0;str[i]!='\0';i++)
                l++;
        printf("length using for loop : %d\n",l);
       printf("length using lib %d\n",n);
}
