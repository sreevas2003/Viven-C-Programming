#include<stdio.h>
void main()
{
        int i,c=0;
        char str[]="sreenivasulu";
        for(i=0;str[i]!='\0';i++)
        {
                if(str[i]=='a'||str[i]=='e'||str[i]=='i'||str[i]=='o'||str[i]=='u'||str[i]=='A'||str[i]=='E'||str[i]=='I'||str[i]=='O'||str[i]=='U')
                        c++;
        }
        printf("count vowels in string is %d\n",c);
}
