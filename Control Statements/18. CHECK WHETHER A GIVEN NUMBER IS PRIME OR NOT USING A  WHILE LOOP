#include<stdio.h>
void main()
{
        int i=2,c=0,n;
        printf("Enter any number :");
        scanf("%d",&n);
        while(i<n/2)
        {
                if(n%i==0)
                {
                        c=1;
                        break;
                }
                i++;
        }
        if(c==0)
                printf("%d is prime \n",n);
        else
                printf("%d is not prime\n",n);
}
