#include<stdio.h>
#include<stdlib.h>
void del(int *a,int,int);
void main()
{
        int i,a[9]={1,2,3,4,5,6,7,8,9},pos;
        printf("Enter delete index position : ");
        scanf("%d",&pos);
        del(a,9,pos);
        for(i=0;i<9-1;i++)
                printf("%d ",a[i]);
        printf("\n");
}
void del(int *a,int s,int pos)
{
        int i;
        for(i=pos;i<s;i++)
                a[i]=a[i+1];
}
