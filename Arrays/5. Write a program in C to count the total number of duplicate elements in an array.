//duplicate numbers
#include<stdio.h>
void main()
{
        int a[8]={2,4,5,3,2,3,2,7};
        int c=0,i=0,j;
        int temp[8];
        while(i<8)
                temp[i++]=-1;
        for(i=0;i<8;i++)
        {
                if(temp[i]==-1)
                {
                        for(j=i+1;j<8;j++)
                        {
                                if(a[i]==a[j])
                                {
                                        c++;
                                        temp[j]=0;
                                }
                        }
                }
        }
        printf("duplicate numbers count is : %d\n",c);
}
