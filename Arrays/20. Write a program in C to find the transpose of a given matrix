//transpose matrix
#include<stdio.h>
void main()
{
        int a[3][3],t[3][3];
        int i,j;
        printf("Enter 3*3 array matrix: ");
        for(i=0;i<3;i++)
        {
                for(j=0;j<3;j++)
                        scanf("%d",&a[i][j]);
        }
        printf("Transpose matrix is :\n");
        for(i=0;i<3;i++)
        {
                for(j=0;j<3;j++)
                        t[i][j]=a[j][i];
        }
        for(i=0;i<3;i++)
        {
                for(j=0;j<3;j++)
                        printf("%d ",t[i][j]);
                printf("\n");
        }
}
