#include<stdio.h>
#include<stdbool.h>
bool distinct(int *a);
void main()
{
        int i,a[10];
        printf("Enter array 10 elements : ");
        for(i=0;i<10;i++)
                scanf("%d",&a[i]);
        if(distinct(a))
                printf("This array is distinct\n");
        else
                printf("This array not distinct\n");
}
bool distinct(int *a)
{
        int i,j;
        for(i=0;i<10;i++)
        {
                for(j=i+1;j<10;j++)
                {
                        if(a[i]==a[j])
                                return false;
                }
        }
        return true;
}
