#include<stdio.h>
void main()
{
        int i,j,a[10]={1,2,3,4,5,6,2,4,5,3};
        printf("repeated elsements are : ");
        for(i=0;i<10;i++)
        {
                for(j=i+1;j<10;j++)
                {
                        if(a[i]==a[j])
                                printf("%d ",a[i]);
                }
        }
}
