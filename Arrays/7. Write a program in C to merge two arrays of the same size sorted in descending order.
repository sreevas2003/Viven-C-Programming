#include<stdio.h>
void main()
{
        int s,i,j,temp;
        printf("Enter size : ");
        scanf("%d",&s);
        int a[s],b[s],k=0,c[50];
        printf("Enter array A : ");
        for(i=0;i<s;i++)
                scanf("%d",&a[i]);
        printf("Enter array B : ");
        for(i=0;i<s;i++)
                scanf("%d",&b[i]);
        for(i=0;i<s;i++)
                c[k++]=a[i];
        for(j=0;j<s;j++)
                c[k++]=b[j];
        for(i=0;i<k;i++)
        {
                for(j=i+1;j<k;j++)
                {
                        if(c[i]<c[j])
                        {
                                temp=c[i];
                                c[i]=c[j];
                                c[j]=temp;
                        }
                }
        }
        for(i=0;i<k;i++)
                printf("%d ",c[i]);
        printf("\n");
}
