#include<stdio.h>
void main()
{
        int sum=0,i,j,a[3][3];
        printf("Enter 3*3 array : ");
        for(i=0;i<3;i++)
        {
                for(j=0;j<3;j++)
                        scanf("%d",&a[i][j]);
        }
        for(i=0;i<3;i++)
        {
                sum+=a[i][i];
        }
        printf("Sum of Right diagonal : %d\n",sum);
}
