#include<stdio.h>
void main()
{
        int i,a[8];
        printf("Enter Array of 8 elements : ");
        for(i=0;i<8;i++)
                scanf("%d",&a[i]);
        int flag,j,k=0;
        for(i=0;i<8;i++)
        {
                flag=1;
                for(j=i+1;j<8;j++)
                {
                        if(a[i]==a[j])
                                flag=0;
                }
                if(flag)
                        a[k++]=a[i];
        }
        printf("After removing duplicate elements : ");
        for(i=0;i<k;i++)
                printf("%d ",a[i]);
}
