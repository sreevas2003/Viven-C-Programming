#include<stdio.h>
void main()
{
        int a[3][3]={1,2,3,4,5,6,7,8,9};
        int b[9],i,j,k=0;
        for(i=0;i<3;i++)
        {
                for(j=0;j<3;j++)
                {
                        b[k++]=a[i][j];
                }
        }
        for(i=0;i<k;i++)
                printf("%d ",b[i]);
        printf("\n");
}
