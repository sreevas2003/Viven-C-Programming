#include<stdio.h>
int symmetric(int a[][3]);
void main()
{
        int a[3][3]={1,2,3,2,4,5,3,5,6};
        if(symmetric(a))
                printf("it is not symmetric\n");
        else
                printf("it is symmetric\n");
}
int symmetric(int a[][3])
{
        int i,j;
        int flag=0;
        for(i=0;i<3;i++)
        {
                for(j=0;j<3;j++)
                {
                        if(a[i][j]!=a[j][i])
                        {
                                flag=1;
                                break;
                        }
                }
        }
        return flag;
}
